{
  "name": "frontend",
  "description": "A1Betting - Real-Time Sports Intelligence Platform",
  "author": "A1Betting",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8173",
    "dev:safe": "cross-env DISABLE_CONSOLE_NINJA=true vite --host 0.0.0.0 --port 8173",
    "dev:minimal": "vite --config vite.config.minimal.ts --host 0.0.0.0 --port 8173",
    "dev:frontend": "vite --host 0.0.0.0 --port 8173",
    "dev:streamlined": "vite --config vite.config.streamlined.ts --host 0.0.0.0 --port 8173",
    "build": "vite build --mode production",
    "build:no-ts": "vite build --config vite.config.minimal.ts --mode production",
    "build:electron": "npm run build && npm run electron:build",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:8173 && electron .\"",
    "electron:build": "electron-builder && node ./scripts/postbuild-copy-package-json.js",
    "electron:pack": "electron-builder --dir",
    "electron:dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 8173",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:debug": "jest --no-coverage --verbose",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:optimization": "jest --testPathPattern=optimization.test"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.21.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/system": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.3",
    "@nestjs/common": "^11.1.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react": "^9.27.0",
    "@tanstack/react-query": "^5.0.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/chart.js": "^2.9.41",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "bottleneck": "^2.19.5",
    "chai": "^5.2.1",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "electron-mocha": "^13.1.0",
    "eventemitter3": "^5.0.1",
    "framer-motion": "^12.23.0",
    "happy-dom": "^18.0.1",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "lucide-react": "^0.515.0",
    "ml-matrix": "^6.12.1",
    "next": "^15.3.4",
    "onnxruntime-node": "^1.22.0-rev",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.2",
    "zod": "^3.25.61",
    "zustand": "^5.0.5"
  },
  "build": {
    "appId": "com.a1betting.app",
    "productName": "A1Betting",
    "directories": {
      "output": "electron-dist"
    },
    "files": [
      "dist/**/*",
      "public/main.js",
      "public/preload.js",
      "main-sportsbook-api.cjs",
      "index.js",
      "knexfile.js",
      "package.json",
      "utils/**/*",
      "node_modules/**/*"
    ],
    "asarUnpack": [
      "utils/**/*",
      "node_modules/winston*/**/*",
      "node_modules/logform/**/*",
      "node_modules/triple-beam/**/*",
      "node_modules/is-stream/**/*",
      "node_modules/readable-stream/**/*",
      "node_modules/knex/**/*"
    ],
    "mac": {
      "category": "public.app-category.sports",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Office"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.9.36",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^30.0.4",
    "electron": "^32.3.3",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-axe": "^10.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  }
}
